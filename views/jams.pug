if (jams.length > 0)
  for jam in jams
    .card.border-primary.m-2.w-18
      .card-body
        if (jam.private)
          i.fa-solid.fa-key.m-1.float-end
        h4.card-title 
          | #{jam.mydate} -&nbsp;#{jam.title}
        h6.card-subtitle.mb-2.text-muted
          if jam.band
            a(href=`javascript:loadEntity('bands', ${jam.band.id});`) #{jam.band.name}
            |  at&nbsp;
          if jam.location
            a(href=`javascript:loadEntity('locations', ${jam.location.id});`) #{jam.location.name}
        p.card-text 
          if (jam.hasOwnProperty('defpic') && jam.defpic !== null && jam.defpic.hasOwnProperty('path') && jam.defpic.path !== null)
            .row
              .col-8
                | #{jam.notes}
              .col-4 
                img.img-thumbnail(src=jam.defpic.path)
          else
            | #{jam.notes}
        button.viewJamButton.ms-2.float-end.btn.btn-primary.btn-sm(data-id=jam.id) View
        if (admin)
          button.editJamButton.ms-2.float-end.btn.btn-secondary.btn-sm(data-id=jam.id) Edit
          button.deleteJamButton.ms-2.float-end.btn.btn-danger.btn-sm.ms-2(data-id=jam.id) Delete
else if (history)
  p No jams occurred on this date in years past.
else
  p There are no jams in BINK! #{history}
